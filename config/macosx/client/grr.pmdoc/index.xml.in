<pkmkdoc spec="1.12">
  <properties>
    <title>%(PackageMaker.Name)s</title>
    <build>%(.dist_dir)s/%(Plist.Binary_Directory)s.pkg</build>
    <organization>%(PackageMaker.Organization)s</organization>
    <userSees ui="both"/>
    <min-target os="3"/>
    <domain system="true"/>
  </properties>
  <distribution>
    <versions min-spec="1.000000"/>
    <scripts></scripts>
  </distribution>
  <contents>
    <choice title="binaries" id="choice%(Plist.Binary_Directory)s" starts_selected="true" starts_enabled="false" starts_hidden="false">
      <pkgref id="%(PackageMaker.Organization)s.%(PackageMaker.Name|lower)s.%(Plist.Binary_Directory)s.pkg"/>
    </choice>
    <choice title="plist" id="choicecom" starts_selected="true" starts_enabled="false" starts_hidden="false">
      <pkgref id="%(PackageMaker.Organization)s.%(PackageMaker.Name|lower)s.%(Plist.Label)s.pkg"/>
    </choice>
  </contents>
  <resources bg-scale="none" bg-align="topleft">
    <locale lang="en"/>
  </resources>
  <flags/>
  <item type="file">01%(Plist.Binary_Directory)s.xml</item>
  <item type="file">02%(Plist.Label_Prefix)s.xml</item>
  <mod>properties.title</mod>
  <mod>properties.anywhereDomain</mod>
  <mod>properties.systemDomain</mod>
</pkmkdoc>
